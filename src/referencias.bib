@Article{Tan2018,
  author       = {Tan, Mei Sze and Chang, Siow-Wee and Cheah, Phaik Leng and Yap, Hwa Jen},
  title        = {Integrative machine learning analysis of multiple gene expression profiles in cervical cancer.},
